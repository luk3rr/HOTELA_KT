					"name": "Customer Register",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Guarda o email e password do request",
									"let requestData = JSON.parse(request.data);",
									"pm.collectionVariables.set(\"customer_email\", requestData.email);",
									"pm.collectionVariables.set(\"customer_password\", requestData.password);",
									"",
									"// Guarda o token, seu tipo e o id de login do cliente",
									"let jsonData = pm.response.json();",
									"pm.collectionVariables.set(\"customer_token\", jsonData.token);",
									"pm.collectionVariables.set(\"customer_token_type\", jsonData.type);",
									"",
									"// Extrai os IDs do token e armazena em variáveis",
									"let token = pm.collectionVariables.get(\"customer_token\");",
									"let payloadBase64 = token.split('.')[1];",
									"let payloadJson = JSON.parse(atob(payloadBase64));",
									"let customerAuthId = payloadJson.authId;",
									"let customerUserId = payloadJson.userId;",
									"pm.collectionVariables.set(\"customer_auth_id\", customerAuthId);",
									"pm.collectionVariables.set(\"customer_user_id\", customerUserId);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
